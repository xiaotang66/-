<title>添加用户</title>
<meta name="keywords" content="">
<meta name="description" content="">
</head>
<body>
<article class="page-container">
	<form action="" method="post" class="form form-horizontal" id="form-member-add">
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-3">省/直辖市：</label>
			<div class="formControls col-xs-8 col-sm-9">
			   <input type="text" class="input-text" placeholder="" name="province" id="province">

			</div>
		</div>
		<div class="row cl">
			<div class="col-xs-8 col-sm-9 col-xs-offset-4 col-sm-offset-3">
				<input class="btn btn-primary radius" type="button" value="&nbsp;&nbsp;提交&nbsp;&nbsp;" id="btn">
			</div>
		</div>
	</form>
</article>

<!--_footer 作为公共模版分离出去-->
<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
<script type="text/javascript" src="../lib/layer/2.4/layer.js"></script>
<script type="text/javascript" src="../static/h-ui/js/H-ui.min.js"></script> 
<script type="text/javascript" src="../static/h-ui.admin/js/H-ui.admin.js"></script> <!--/_footer 作为公共模版分离出去-->

<!--请在下方写此页面业务相关的脚本-->
<script type="text/javascript" src="../lib/My97DatePicker/4.8/WdatePicker.js"></script> 
<script type="text/javascript" src="../lib/datatables/1.10.0/jquery.dataTables.min.js"></script> 
<script type="text/javascript" src="../lib/laypage/1.2/laypage.js"></script>
<script type="text/javascript">
 var sesspcaid = sessionStorage.getItem('pacid');
 if(sesspcaid==""){
 	$("#btn").click(function(){
 	$.ajax({
	      type: 'post',
	      url: 'http://47.100.200.62:8760/v0.1/base/pca/add',
	      data: {
	        parent_Pca:0,
	        pca_Flag:0,
	        pca_Name:$("#province").val()
	      },
	      success: function(data){
	      	if(data.success==true){
	      		layer.msg("添加成功！")
	      	}
	      	else{
	      		layer.msg(data.msg)
	      	}
	       }
	 })

 })
 
 }
 else{
 	var status ="";
 	$.ajax({
 	url:"http://47.100.200.62:8760/v0.1/area/",
 	type:"get",
 	data:{
 		parentPca:0
 	},
 	success:function(data){
 		for(var i=0;i<data.data.length;i++)
 		if(data.data[i].pcaId==sesspcaid){
           $("#province").val(data.data[i].pcaName)
           status = data.data[i].status;
 		}
 	}
 })
 	$("#btn").click(function(){
 	$.ajax({
	      type: 'post',
	      url: 'http://47.100.200.62:8760/v0.1/base/pca/update',
	      data: {
	      	pca_Id:sesspcaid,
	        parent_Pca:0,
	        pca_Flag:0,
	        pca_Name:$("#province").val(),
	        status:status
	      },
	      success: function(data){
	      	  if(data.success==true){
		        	layer.msg("修改成功",{icon:1,time:1000})
		        }
		       else {
		         layer.msg('data.msg',{icon:1,time:1000});
		       }
	       }
	 })

 })
 }
 
</script> 
<!--/请在上方写此页面业务相关的脚本-->
</body>
</html>